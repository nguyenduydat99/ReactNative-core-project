{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "components/*": ["src/components/*"],
      "adapters/*": ["src/adapters/*"],
      "screens/*": ["src/screens/*"],
      "interfaces/*": ["src/interfaces/*"],
      "server/*": ["src/server/*"],
      "sevices/*": ["src/services/*"],
      "styles/*": ["src/styles/*"],
      "utils/*": ["src/utils/*"]
    },
    "target": "esnext",
    "module": "commonjs",
    "lib": ["es6", "es2017"],
    "allowJs": true,
    "jsx": "react-native",
    "noEmit": true,
    "isolatedModules": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "dist"
  ],
  "extends": "expo/tsconfig.base"
}
